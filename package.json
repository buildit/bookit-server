{
  "name": "projects",
  "version": "0.0.0",
  "description": "projects",
  "license": "MIT",
  "repository": "",
  "author": "",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && tsc --version && tsc --pretty",
    "test:unit": "npm run build && ROOT=`pwd` && NODE_ENV=unit-test NODE_CONFIG_DIR=$ROOT/lib/config mocha --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "test:int": "npm run build && ROOT=`pwd` && NODE_ENV=integration-test NODE_CONFIG_DIR=$ROOT/lib/config mocha --timeout=60000 --compilers ts:ts-node/register --recursive 'test-int/**/*-spec.ts'",
    "test": "npm run test:unit && npm run test:int",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "start": "ROOT=`pwd` && NODE_ENV=development NODE_CONFIG_DIR=$ROOT/lib/config nodemon --delay 2.5 --watch lib lib/rest/index.js",
    "dev": "concurrently --kill-others 'npm run watch' 'npm run start'"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^0.4.1",
    "config": "^1.25.1",
    "cors": "^2.8.3",
    "cwait": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^0.4.1",
    "@types/chai": "^3.0.0",
    "@types/config": "0.0.32",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/log4js": "0.0.32",
    "@types/mocha": "^2.0.0",
    "@types/node": "^6.0.68",
    "@types/request": "0.0.42",
    "@types/supertest": "^2.0.0",
    "chai": "^3.0.0",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "log4js": "^1.1.1",
    "mocha": "^3.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "request": "^2.81.0",
    "rimraf": "^2.0.0",
    "supertest": "^3.0.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
